{
  "expo": {
    "name": "NativeRaceTimer",
    "slug": "nativeracetimer",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Capstone project for Epicodus programming bootcamp. Written in  react-native with Firebase authentication and Firestore nosql data storage",
    "githubUrl": "https://github.com/neilbateman/NativeRaceTimer"
  }
}
